{
  "name": "prmptart",
  "version": "1.0.0",
  "description": "A comprehensive RTCCF-compliant prompt library for product designers with beautiful UI, dark mode, and animation support",
  "type": "module",
  "private": false,
  "keywords": [
    "prompt-library",
    "design-prompts",
    "rtccf",
    "figma",
    "react",
    "typescript",
    "tailwindcss",
    "ui-components"
  ],
  "author": "Stephen Webb",
  "license": "MIT",
  "homepage": "https://github.com/your-username/figma-prompt-library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/figma-prompt-library.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/figma-prompt-library/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "embla-carousel-react": "^8.0.0",
    "framer-motion": "^12.23.12",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.294.0",
    "motion": "^10.16.2",
    "next-themes": "^0.2.1",
    "react": "^18.0.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.0.0",
    "react-hook-form": "7.55.0",
    "react-resizable-panels": "^0.0.63",
    "recharts": "^2.8.0",
    "sonner": "2.0.3",
    "tailwind-merge": "^2.0.0",
    "vaul": "^0.9.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.5.6",
