# Environment Configuration
VITE_APP_NAME=Figma Prompt Library
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=RTCCF-compliant prompt library for product designers

# Theme Configuration  
VITE_DEFAULT_THEME=system
VITE_THEME_STORAGE_KEY=figma-prompt-library-theme

# Features Flags (optional)
VITE_ENABLE_CUSTOM_PROMPTS=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_EXPORT=true

# API Configuration (if needed for future features)
VITE_API_URL=https://api.example.com
VITE_API_VERSION=v1

# Deployment
VITE_PUBLIC_URL=/
VITE_BUILD_PATH=dist